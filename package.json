{
  "name": "folder-path-color-auto",
  "displayName": "Folder Path Color Auto",
  "publisher": "jjhidalgar",
  "description": "All subfolders within a specified path will be assigned a color for easy differentiation. You can generate automatic configurations for each .git project.",
  "version": "0.0.2",
  "icon": "/images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/jacob-j/vscode-folder-path-color"
  },
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "properties": {
        "folder-path-color.folders": {
          "type": "array",
          "default": [],
          "items": {
            "type": "object",
            "properties": {
              "path": {
                "type": "string"
              },
              "color": {
                "type": "string"
              },
              "symbol": {
                "type": "string"
              },
              "tooltip": {
                "type": "string"
              }
            }
          },
          "description": "Array of folder path and color pairs to be colored."
        }
      }
    },
    "colors": [
      {
        "id": "folderPathColor.b1",
        "description": "Assignable custom color 1 for the Folder Path Color extension",
        "defaults": {
          "dark": "#ABCDEF",
          "light": "#92abff"
        }
      },
      {
        "id": "folderPathColor.b2",
